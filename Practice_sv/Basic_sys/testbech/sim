:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x632776933270 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x632776933400 .scope module, "d_ff_tb" "d_ff_tb" 3 3;
 .timescale -9 -12;
v0x632776947c80_0 .var "clk", 0 0;
v0x632776947d40_0 .var "d", 0 0;
v0x632776947e10_0 .net "q", 0 0, v0x632776947a40_0;  1 drivers
v0x632776947f10_0 .var "rst", 0 0;
S_0x6327768f9490 .scope module, "d_ff0" "d_ff" 3 12, 4 1 0, S_0x632776933400;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x6327768f96b0_0 .net "clk", 0 0, v0x632776947c80_0;  1 drivers
v0x632776947980_0 .net "d", 0 0, v0x632776947d40_0;  1 drivers
v0x632776947a40_0 .var "q", 0 0;
v0x632776947b10_0 .net "rst", 0 0, v0x632776947f10_0;  1 drivers
E_0x6327768f9910/0 .event negedge, v0x632776947b10_0;
E_0x6327768f9910/1 .event posedge, v0x6327768f96b0_0;
E_0x6327768f9910 .event/or E_0x6327768f9910/0, E_0x6327768f9910/1;
    .scope S_0x6327768f9490;
T_0 ;
    %wait E_0x6327768f9910;
    %load/vec4 v0x632776947b10_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x632776947a40_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x632776947980_0;
    %assign/vec4 v0x632776947a40_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x632776933400;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x632776947c80_0, 0;
    %end;
    .thread T_1;
    .scope S_0x632776933400;
T_2 ;
    %delay 1000, 0;
    %load/vec4 v0x632776947c80_0;
    %inv;
    %assign/vec4 v0x632776947c80_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x632776933400;
T_3 ;
    %vpi_call/w 3 24 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call/w 3 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x632776933400 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x632776947f10_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x632776947d40_0, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x632776947f10_0, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x632776947d40_0, 0;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x632776947d40_0, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x632776947d40_0, 0;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x632776947d40_0, 0;
    %end;
    .thread T_3;
    .scope S_0x632776933400;
T_4 ;
    %vpi_call/w 3 38 "$monitor", "monitor: time=%t,d=%b,q=%b", $time, v0x632776947d40_0, v0x632776947e10_0 {0 0 0};
    %delay 2000000, 0;
    %vpi_call/w 3 39 "$finish" {0 0 0};
    %end;
    .thread T_4;
