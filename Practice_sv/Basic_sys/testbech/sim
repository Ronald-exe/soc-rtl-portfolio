#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5d6964dd8270 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5d6964dd8400 .scope module, "d_ff_tb" "d_ff_tb" 3 3;
 .timescale -9 -12;
v0x5d6964ded9a0_0 .var "clk", 0 0;
v0x5d6964deda60_0 .var "d", 0 0;
v0x5d6964dedb30_0 .net "q", 0 0, v0x5d6964ded760_0;  1 drivers
v0x5d6964dedc30_0 .var "rst", 0 0;
S_0x5d6964d9e490 .scope task, "appli_rst" "appli_rst" 3 24, 3 24 0, S_0x5d6964dd8400;
 .timescale -9 -12;
TD_d_ff_tb.appli_rst ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d6964dedc30_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d6964deda60_0, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5d6964dedc30_0, 0;
    %end;
S_0x5d6964d9e620 .scope module, "d_ff0" "d_ff" 3 12, 4 1 0, S_0x5d6964dd8400;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x5d6964dd8590_0 .net "clk", 0 0, v0x5d6964ded9a0_0;  1 drivers
v0x5d6964ded6a0_0 .net "d", 0 0, v0x5d6964deda60_0;  1 drivers
v0x5d6964ded760_0 .var "q", 0 0;
v0x5d6964ded830_0 .net "rst", 0 0, v0x5d6964dedc30_0;  1 drivers
E_0x5d6964d9e910/0 .event negedge, v0x5d6964ded830_0;
E_0x5d6964d9e910/1 .event posedge, v0x5d6964dd8590_0;
E_0x5d6964d9e910 .event/or E_0x5d6964d9e910/0, E_0x5d6964d9e910/1;
    .scope S_0x5d6964d9e620;
T_1 ;
    %wait E_0x5d6964d9e910;
    %load/vec4 v0x5d6964ded830_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d6964ded760_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5d6964ded6a0_0;
    %assign/vec4 v0x5d6964ded760_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5d6964dd8400;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5d6964ded9a0_0, 0;
    %end;
    .thread T_2;
    .scope S_0x5d6964dd8400;
T_3 ;
    %delay 1000, 0;
    %load/vec4 v0x5d6964ded9a0_0;
    %inv;
    %assign/vec4 v0x5d6964ded9a0_0, 0;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5d6964dd8400;
T_4 ;
    %vpi_call/w 3 33 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call/w 3 34 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d6964dd8400 {0 0 0};
    %fork TD_d_ff_tb.appli_rst, S_0x5d6964d9e490;
    %join;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5d6964deda60_0, 0;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d6964deda60_0, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5d6964deda60_0, 0;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d6964deda60_0, 0;
    %end;
    .thread T_4;
    .scope S_0x5d6964dd8400;
T_5 ;
    %vpi_call/w 3 44 "$monitor", "monitor: time=%t,d=%b,q=%b", $time, v0x5d6964deda60_0, v0x5d6964dedb30_0 {0 0 0};
    %delay 200000, 0;
    %vpi_call/w 3 45 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 5;
